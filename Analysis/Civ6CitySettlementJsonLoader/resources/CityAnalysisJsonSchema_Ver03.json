          "housing": {
            "$id": "#/properties/cityPerTurnLog/items/properties/housing",
            "type": "integer",
            "title": "The Housing Schema"
          },
          "cityName": {
            "$id": "#/properties/cityPerTurnLog/items/properties/cityName",
            "type": "string",
            "title": "The Cityname Schema",
            "pattern": "^(.*)$"
          },
          "amenitiesNeeded": {
            "$id": "#/properties/cityPerTurnLog/items/properties/amenitiesNeeded",
            "type": "integer",
            "title": "The Amenitiesneeded Schema"
