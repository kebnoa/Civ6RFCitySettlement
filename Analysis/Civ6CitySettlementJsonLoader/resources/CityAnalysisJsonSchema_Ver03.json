          "foodPerTurn": {
            "$id": "#/properties/cityPerTurnLog/items/properties/foodPerTurn",
            "type": "string",
            "title": "The Foodperturn Schema",
            "pattern": "^[+-]*(\\d*\\.)?\\d+$"
          },
          "productionPerTurn": {
            "$id": "#/properties/cityPerTurnLog/items/properties/productionPerTurn",
            "type": "string",
            "title": "The Productionperturn Schema",
            "pattern": "^[+-]*(\\d*\\.)?\\d+$"
          },
