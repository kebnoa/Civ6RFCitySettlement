            "title": "The Goldperturn Schema",
            "pattern": "^[+-]*(\\d*\\.)?\\d+$"
          },
          "ownerCiv": {
            "$id": "#/properties/cityPerTurnLog/items/properties/ownerCiv",
            "type": "string",
            "title": "The Ownerciv Schema",
            "pattern": "^(.*)$"
          },
          "scienceToolTip": {
            "$id": "#/properties/cityPerTurnLog/items/properties/scienceToolTip",
            "type": "string",
            "title": "The Sciencetooltip Schema",
            "pattern": "^(.*)$"
          },
          "productionToolTip": {
            "$id": "#/properties/cityPerTurnLog/items/properties/productionToolTip",
            "type": "string",
            "title": "The Productiontooltip Schema",
            "pattern": "^(.*)$"
          },
          "ownerName": {
            "$id": "#/properties/cityPerTurnLog/items/properties/ownerName",
            "type": "string",
            "title": "The Ownername Schema",
            "pattern": "^(.*)$"
