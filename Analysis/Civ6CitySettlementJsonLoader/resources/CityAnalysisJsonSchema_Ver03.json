                "resourceCount": {
                  "$id": "#/properties/citySettledLog/items/properties/plots/items/properties/resourceCount",
                  "type": "integer",
                  "title": "The Resourcecount Schema"
                },
                "x": {
                  "$id": "#/properties/citySettledLog/items/properties/plots/items/properties/x",
                  "type": "integer",
                  "title": "The X Schema"
                },
                "resource": {
                  "$id": "#/properties/citySettledLog/items/properties/plots/items/properties/resource",
                  "type": "string",
                  "title": "The Resource Schema",
                  "pattern": "^(.*)$"
