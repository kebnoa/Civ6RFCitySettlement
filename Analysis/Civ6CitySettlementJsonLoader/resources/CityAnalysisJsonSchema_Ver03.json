          "turn": {
            "$id": "#/properties/cityPerTurnLog/items/properties/turn",
            "type": "integer",
            "title": "The Turn Schema"
          },
          "foodToolTip": {
            "$id": "#/properties/cityPerTurnLog/items/properties/foodToolTip",
            "type": "string",
            "title": "The Foodtooltip Schema",
            "pattern": "^(.*)$"
          },
          "population": {
            "$id": "#/properties/cityPerTurnLog/items/properties/population",
            "type": "integer",
            "title": "The Population Schema"
          },
          "culturePerTurn": {
            "$id": "#/properties/cityPerTurnLog/items/properties/culturePerTurn",
            "type": "string",
            "title": "The Cultureperturn Schema",
            "pattern": "^[+-]*(\\d*\\.)?\\d+$"
