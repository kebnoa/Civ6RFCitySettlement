            "$id": "#/properties/cityPerTurnLog/items/properties/amenities",
            "type": "integer",
            "title": "The Amenities Schema"
          },
          "faithToolTip": {
            "$id": "#/properties/cityPerTurnLog/items/properties/faithToolTip",
            "type": "string",
            "title": "The Faithtooltip Schema",
            "pattern": "^(.*)$"
          },
          "cultureToolTip": {
            "$id": "#/properties/cityPerTurnLog/items/properties/cultureToolTip",
            "type": "string",
            "title": "The Culturetooltip Schema",
            "pattern": "^(.*)$"
